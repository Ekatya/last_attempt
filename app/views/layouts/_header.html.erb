<header class="navbar navbar-fixed-top navbar-inverse"> <!-- тегу добавлены три css класса, разделенных пробелом -->
  <div class="navbar-inner">
    <div class="container">
      <%= link_to "Logotip", '#', id: "logo" %>
      <nav>
        <ul class="nav pull-right">
          <li><%= link_to "Home", root_path %></li> <!-- список навигационных ссылок-->
          <li><%= link_to "Help", '#' %></li>
          
          <% unless current_user %>
          <li><%= link_to "Sign in", signin_path %></li>
          <% else %>
          <li><%= link_to "Sign out", signout_path %></li>
          <% end %>
          
        </ul>
      </nav>
    </div>
  </div>
</header>